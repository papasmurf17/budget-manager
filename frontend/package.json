{
  "name": "budget-manager",
  "version": "0.10.0",
  "description": "A webapp to track all income and expenses",
  "private": false,
  "license": "MIT",
  "keywords": [
    "budget manager",
    "budget"
  ],
  "engines": {
    "node": "10.x"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.0",
    "@welld/react-components": "^0.6.2-RC.192",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-link-state": "^0.4.2",
    "classnames": "^2.2.6",
    "date-fns": "^1.30.1",
    "dotenv": "^8.0.0",
    "formik": "^1.5.7",
    "graphql": "^14.5.4",
    "graphql-tag": "^2.10.1",
    "keycloak-js": "4.8.3",
    "lodash": "^4.17.15",
    "react": "^16.8.3",
    "react-apollo": "^3.1.0",
    "react-dom": "^16.8.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "3.1.0",
    "react-use": "^12.2.0",
    "redux-thunk": "^2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.welld.io/schindler/figs/ui.git"
  },
  "bugs": {
    "url": "https://gitlab.welld.io/schindler/figs/ui/issues"
  },
  "author": "Matteo Codogno <matteo.codogno@welld.ch>",
  "files": [
    "build/"
  ],
  "publishConfig": {
    "registry": "https://nexus.welld.io/repository/npm/"
  },
  "scripts": {
    "start": "craco start",
    "start:prod": "node server.js",
    "build": "craco build && yarn pack",
    "heroku-postbuild": "craco build",
    "test": "cross-env CI=true craco test --no-cache",
    "test:watch": "craco test \"^(?!.*e2e\\.spec\\.js$).*\\.spec\\.js$\" --no-cache",
    "test:unit": "yarn test \"^(?!.*e2e\\.spec\\.js$).*\\.spec\\.js$\"",
    "test:e2e": "yarn test --config ./setupIntegrationTests.js \"(.+)\\.e2e\\.spec\\.js$\"",
    "lint:all": "eslint src",
    "quality": "yarn sonar-scanner",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.css": [
      "stylelint"
    ]
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.0",
    "@craco/craco": "^3.4.1",
    "cross-env": "^5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.5.0",
    "faker": "^4.1.0",
    "husky": "^1.3.1",
    "jest-puppeteer": "3.9.0",
    "jest-sonar-reporter": "^2.0.0",
    "light-server": "2.6.0",
    "lint-staged": "^8.1.4",
    "postcss": "^7.0.14",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "prop-types": "^15.7.2",
    "puppeteer": "^1.12.2",
    "react-test-renderer": "^16.8.4",
    "sonar-scanner": "^3.1.0",
    "stylelint": "^9.10.1",
    "tailwindcss": "^1.0.3",
    "unfetch": "^4.1.0",
    "waait": "^1.0.4"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
